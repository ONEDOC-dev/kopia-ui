{
  "appId": "com.onedoc.onecloud",
  "productName": "oneCLOUD",
  "files": [
    ".vite/build/**/*",
    "resources/**/*",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "resources",
      "to": "server",
      "filter": [
        "**/*"
      ]
    }
  ],
  "nsis": {
    "oneClick": false,
    "perMachine": false,
    "artifactName": "$productName-$version.exe"
  },
  "win": {
    "target": [
      "nsis",
      "zip"
    ]
  },
  "mac": {
    "hardenedRuntime": false,
    "gatekeeperAssess": false,
    "category": "public.app-category.utilities",
    "target": [
      {
        "target": "dir",
        "arch": [
          "arm64"
        ]
      }
    ]
  }
}