{
  "appId": "com.onedoc.onecloud",
  "productName": "oneCLOUD",
  "files": [
    "output/electron/**/*",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "resources",
      "to": "resources",
      "filter": [
        "**/*"
      ]
    }
  ],
  "mac": {
    "target": [
      {
        "target": "dmg",
        "arch": [
          "arm64"
        ]
      },
      {
        "target": "zip",
        "arch": [
          "arm64"
        ]
      }
    ]
  }
}